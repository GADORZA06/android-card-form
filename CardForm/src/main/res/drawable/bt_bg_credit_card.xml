<?xml version="1.0" encoding="utf-8"?>
<ripple xmlns:android="http://schemas.android.com/apk/res/android"
    android:color="@color/bt_ripple">
    <item android:state_enabled="true">
        <shape android:shape="rectangle">
            <corners android:radius="4dp" />
            <solid android:color="@color/bt_white" />
            <stroke
                android:width="1dp"
                android:color="@color/bt_bg_edit_text_default" />
        </shape>
    </item>
    <item android:drawable="@drawable/bt_ic_credit_card"
        android:top="10dp"
        android:left="10dp"
        android:right="10dp"
        android:bottom="10dp"/>
</ripple>
